        var radioValue = $("input[name='have_film']:checked").val();
        if (radioValue == 'have_film') {
           calcPrice()
        } else{
           calcPrice()
        }
    });

    $("#have_spring").click(function() {

        var radioValue = $("input[name='have_spring']:checked").val();
        if (radioValue == 'have_spring') {
           calcPrice()
        } else{
           calcPrice()
        }
    });

