        // Input params
        switchQuantity = $("#lube_quantity").val();
        addFilm = $("input[name='film']:checked").val();
        addSpring = $("input[name='spring']:checked").val();


        if(addFilm == 'add_film'){
            priceFilm = current_price_film
        }else{
